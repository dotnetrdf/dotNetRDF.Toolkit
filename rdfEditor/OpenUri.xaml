<Window x:Class="rdfEditor.OpenUri"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Open URI" Height="91" Width="424" Loaded="Window_Loaded">
    <DockPanel Height="50">
        <Label DockPanel.Dock="Left">URI:</Label>
        <TextBox Name="txtUri" MaxLines="1" DockPanel.Dock="Top">http://</TextBox>
        <Button Name="btnOpen" DockPanel.Dock="Bottom" Click="btnOpen_Click">Open URI</Button>
    </DockPanel>
</Window>
